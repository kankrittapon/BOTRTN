{
  "TargetUrl": "https://www.facebook.com/",
  "Headless": true,
  "Browser": "chromium",
  "UseInstalledChromeChannel": false,
  "ScreenshotPath": "artifacts/screenshot.png",
  "UserDataDirName": "botRTN",
  "Timeout": 30000,

  "Proxy": {
    "Enabled": false,
    "Server": "",
    "Username": "",
    "Password": ""
  },

  "Credentials": {
    "User": "",
    "Pass": ""
  },

  "Login": {
    "Url": "https://www.facebook.com/login",
    "UserSelector": "input[name='email']",
    "PassSelector": "input[name='pass']",
    "SubmitSelector": "button[name='login'], button[type='submit']",
    "LoggedInCheckSelector": "div[role='feed']",

    "AfterLoginWaitSelector": "div[role='feed']",

    "AfterLoginUrl": "https://www.facebook.com/",
    "TwoFactorSelector": "input#approvals_code, input[name='approvals_code'], input[name='otp']",
    "WaitTimeout": 30000
  }
}
